diff --git a/newlib/libc/include/math.h b/newlib/libc/include/math.h
index 2a322ee26..68c1b1074 100644
--- a/newlib/libc/include/math.h
+++ b/newlib/libc/include/math.h
@@ -305,10 +305,29 @@ extern double tgamma _PARAMS((double));
 extern double nearbyint _PARAMS((double));
 extern long int lrint _PARAMS((double));
 extern long long int llrint _PARAMS((double));
+#ifdef __PRU__
+extern double __pruabi_nround _PARAMS((double));
+/* libc++ requires a real function, so cannot use a macro.  */
+static inline __attribute__((always_inline))
+double round (double a)
+{
+  return __pruabi_nround(a);
+}
+#else
 extern double round _PARAMS((double));
+#endif
 extern long int lround _PARAMS((double));
 extern long long int llround _PARAMS((double));
+#ifdef __PRU__
+extern double __pruabi_trunc _PARAMS((double));
+static inline __attribute__((always_inline))
+double trunc (double a)
+{
+  return __pruabi_trunc(a);
+}
+#else
 extern double trunc _PARAMS((double));
+#endif
 extern double remquo _PARAMS((double, double, int *));
 extern double fdim _PARAMS((double, double));
 extern double fmax _PARAMS((double, double));
@@ -375,10 +394,28 @@ extern float tgammaf _PARAMS((float));
 extern float nearbyintf _PARAMS((float));
 extern long int lrintf _PARAMS((float));
 extern long long int llrintf _PARAMS((float));
+#ifdef __PRU__
+extern float __pruabi_roundf _PARAMS((float));
+static inline __attribute__((always_inline))
+float roundf (float a)
+{
+  return __pruabi_roundf(a);
+}
+#else
 extern float roundf _PARAMS((float));
+#endif
 extern long int lroundf _PARAMS((float));
 extern long long int llroundf _PARAMS((float));
+#ifdef __PRU__
+extern float __pruabi_truncf _PARAMS((float));
+static inline __attribute__((always_inline))
+float truncf (float a)
+{
+  return __pruabi_truncf(a);
+}
+#else
 extern float truncf _PARAMS((float));
+#endif
 extern float remquof _PARAMS((float, float, int *));
 extern float fdimf _PARAMS((float, float));
 extern float fmaxf _PARAMS((float, float));
